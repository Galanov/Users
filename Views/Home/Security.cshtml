@using ManyToManyExample.Models
@model IEnumerable<SecurityPolicy>
@{
    ViewBag.Title = "Security Page";
}
<h2>Список пользователей</h2>
<table>
    <tr>
        <th>Права доступа</th>
        <th></th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name);
            </td>
            <td>
                @Html.ActionLink("Посмотреть пользователей", "SecurityInfo", new { id = item.Id })
            </td>
        </tr>
    }
</table>